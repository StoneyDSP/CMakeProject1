{
  "name": "cmakeproject1",
  "version-string": "latest",
  "builtin-baseline": "31c43c228ce0879a0b49d5e081dddb4e4827a80b",
  "maintainers": [
    "StoneyDSP"
  ],
  "documentation": "https://github.com/StoneyDSP/CMakeProject1/README.md",
  "homepage": "https://github.com/StoneyDSP/CMakeProject1",
  "license": "MIT",
  "description": [
    "Generic CMake Project template for generating a full CI project pipeline, with in-built CMake presets, vcpkg package dependancy manager, NuGet binary caching, git actions workflows, and html documetation generated by doxygen integration; plus a whole lot more to come."
  ],
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-cmake-get-vars",
      "host": true
    },
    {
      "name": "catch2",
      "host": true
    },
    {
      "name": "fmt",
      "host": true
    }
  ],
  "features": {
    "foo": {
      "description": [
        "Third-party library used for printing to terminal"
      ],
      "dependencies": [
        "fmt"
      ]
    },
    "tests": {
      "description": [
        "Cpp Unit Testing Framework"
      ],
      "dependencies": [
        "catch2"
      ]
    },
    "main": {
      "description": [
        "Defines an 'int main()' for program execution"
      ]
    }
  },
  "default-features": [
    "main"
  ]
}
